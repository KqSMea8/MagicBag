<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>argtree Web Application</display-name>
    <servlet>
        <servlet-name>argTreeReflectServlet</servlet-name>
        <servlet-class>org.github.helixcs.argtree.servlet.ArgTreeReflectServlet</servlet-class>
        <init-param>
            <param-name>env</param-name>
            <param-value>dev</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>argTreeReflectServlet</servlet-name>
        <url-pattern>/argTreeReflectServlet/*</url-pattern>
    </servlet-mapping>

</web-app>
